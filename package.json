	"scripts": {
		"//// Yarn ////////////////////////////////////////////////////////////////////": "",
		"upgrade-all-packages-and-install": "yarn upgrade && syncyarnlock -s -k -a latest && yarn install && prettier --write 'ui_components/**/*.{js,json}' '*.{json,js}' && lerna clean --yes",
		"interactive-package-upgrade-view": "yarn upgrade-interactive --latest",
		"list-outdated-packages": "yarn outdated",
		"//// Utility ////////////////////////////////////////////////////////////////////": "",
		"remove-node-modules-folders": "lerna clean --yes",
		"//// Storybook ////////////////////////////////////////////////////////////////////": "",
		"run-storybook": "yarn install && lerna clean --yes && start-storybook -p 6006 --no-manager-cache -s .storybook/shared_assets",
		"build-storybook": "yarn install && lerna clean --yes && build-storybook -s .storybook/shared_assets -o built_storybook",
		"run-storybook-build-ci": "http-server built_storybook -p 6006",
		"//// Lint & Prettier ////////////////////////////////////////////////////////////////////": "",
		"eslint": "eslint ui_components/**/*.js ui_components/**/docs.mdx cypress/integration/**/*.spec.js",
		"eslint-fix": "eslint ui_components/**/*.js ui_components/**/docs.mdx cypress/integration/**/*.spec.js --fix",
		"prettier": "prettier --write 'ui_components/**/*.{js,json,mdx}' 'cypress/**/*.{js,json}' '*.{json,js}'",
		"stylelint": "stylelint 'ui_components/**/*.js'",
		"//// Lerna /////////////////////////////////////////////////////////////////////": "",
		"build-component-packages": "yarn install && lerna exec --parallel -- rm -rf ./dist ./node_modules && lerna exec --parallel -- rollup -c=$INIT_CWD/rollup.config.js && yarn run remove-node-modules-folders",
		"publish-to-npm": "lerna publish from-package",
		"build-component-packages-publish-to-npm": "yarn install && lerna exec --parallel -- rm -rf ./dist ./node_modules && lerna exec --parallel -- rollup -c=$INIT_CWD/rollup.config.js && lerna publish from-package",
		"install-and-link-packages": "lerna bootstrap --use-workspaces && lerna clean --yes",
		"list-packages": "lerna ls --all --long",
		"list-publishable-packages": "lerna changed --long",
		"bump-patch-version-all-packages-no-git-remote-push": "lerna version patch --no-push",
		"show-difference-in-packages": "lerna diff",
		"force-build-publish": "lerna exec --parallel -- babel --root-mode upward index.js -d dist --ignore **/*.stories.js,**/*.mdx && lerna publish from-package",
		"//// Percy /////////////////////////////////////////////////////////////////////": "",
		"locally-build-storybook-run-percy": "build-storybook && export PERCY_TOKEN=da131d85ec56cca2ff398ed2351115b8de7add85109de165a67093370fc9a7c3 && export PERCY_BRANCH=local && percy-storybook --widths=1024 --build_dir=built_storybook",
		"percy-in-ci-mol-desktop": "export PERCY_TOKEN=da131d85ec56cca2ff398ed2351115b8de7add85109de165a67093370fc9a7c3 PERCY_TARGET_BRANCH=dev && percy-storybook --widths=1024 --build_dir=built_storybook",
		"percy-in-ci-mol-mobile": "export PERCY_TOKEN=0d95685750c9e98a9459a413b6bfd8727e0659a2089931df6890a2777f8b2c03 && PERCY_TARGET_BRANCH=dev && percy-storybook --widths=1024 --build_dir=built_storybook",
		"percy-in-ci-vms": "export PERCY_TOKEN=c3d1791363cc67fceed7353191832e58ec8335b2e28509cbcff6a30ead2471b1 PERCY_TARGET_BRANCH=dev && percy-storybook --widths=1024 --build_dir=built_storybook",
		"percy-in-ci-mok": "export PERCY_TOKEN=a3d4a3ff355515146e6901abb4e450ea965878ba29ca8a120f0066f52c61279c PERCY_TARGET_BRANCH=dev && percy-storybook --widths=1024 --build_dir=built_storybook",
		"percy-in-ci-mod": "export PERCY_TOKEN=57ecefd5e03a75ab4db831b4f519a625f759616a6196b25e473da10a56cbdc5b && PERCY_TARGET_BRANCH=dev && percy-storybook --widths=1024 --build_dir=built_storybook",
		"//// Cypress /////////////////////////////////////////////////////////////////////": "",
		"cypress-open-gui-local": "cypress open",
		"is-cypress-working": "cypress verify && cypress info && cypress version && cypress cache list",
		"cypress-run-mol-desktop": "cypress run --browser chrome --headless --config testFiles='{storybook.spec.js,mol-desktop/**/*.spec.js}'",
		"cypress-run-mol-mobile": "cypress run --browser chrome --headless --config testFiles='{storybook.spec.js,mol-mobile/**/*.spec.js}'",
		"cypress-run-mod": "cypress run --browser chrome --headless --config testFiles='{storybook.spec.js,mod/**/*.spec.js}'",
		"cypress-run-vms": "cypress run --browser chrome --headless --config testFiles='{storybook.spec.js,vms/**/*.spec.js}'",
		"cypress-run-mok": "cypress run --browser chrome --headless --config testFiles='{storybook.spec.js,mok/**/*.spec.js}'",
		"cypress-run-utils": "cypress run --browser chrome --headless --config testFiles='{storybook.spec.js,utils/**/*.spec.js}'",
		"prepare": "husky install",
		"//// Code Sandbox /////////////////////////////////////////////////////////////////////": "",
		"code-sandbox-run-tests": "./scripts/runCodesandboxTests.sh"
	},
	"engines": {
		"node": ">=10.13.0",
		"yarn": ">=1.22.4"
	}
